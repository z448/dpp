export CYPM=~/.cypm
export CYPOOL=$CYPM/pool
export CYBIN=$CYPM/.bin
alias pool="cd $CYPOOL"

export PATH=$PWD:$PWD/bin:$PATH
export PERL5LIB=$PWD/local/lib/perl5:$PWD/lib:$PERL5LIB
export PERL5LIB=$CYPOOL/usr/local/lib/perl5/lib/perl5:$CYPOOL/usr/local/lib/perl5/lib/perl5/arm-iphoneos-2level:$PERL5LIB
export PATH=$CYPM/pool/usr/local/lib/perl5/bin:$PATH
    

export PATH=/usr/local/bin:$PATH
export PS_PATH=/usr/bin/ps

export PATH=$CYBIN:$PATH

alias cypan_test='cpanm --reinstall -l build/usr/local/lib/perl5 Pegex'

    echo -n '   - to install dependencies use Carton '
    echo -e "\n\e[33mcarton install \e[0m"
    echo -n '   - if you dont have it install it via cpan '
    echo -e "\n\e[33mcpan Carton \e[0m"
    echo '   - anytime you want to reset env, do '
    echo -e "\e[31msetup \e[0m\n\n"
