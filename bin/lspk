#!/bin/bash

. ~/.cypm/.env

if [ "$1" = "-d" ]; then
    perl -E 'push @l,qx!find . | grep packlist > pack.list!; qx!tar -zcvf packlists.tgz -T pack.list!;'
    mv pack $CYPM/.stash/packlist
    cd $CYPM/.stash/packlist
    tar -xvf "packlists.tgz"
    cd ../../pool
    echo "moving packlist into stash\n"
    find . | grep packlist | grep -v $2
    find . | grep packlist | grep -v $2 > p
    while read i;do rm $i;done < p
    rm p
else
    find . | grep packlist 
fi


